<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Build Project">
        <Item>
          <IdeaActionID>CompileDirty</IdeaActionID>
          <ShortCut>'Ctrl+F9'</ShortCut>
          <Description>Build Project</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Delete...">
        <Item>
          <IdeaActionID>$Delete</IdeaActionID>
          <ShortCut>'Delete'</ShortCut>
          <Description>Delete...</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Implementation(s)">
        <Item>
          <IdeaActionID>GotoImplementation</IdeaActionID>
          <ShortCut>'Ctrl+Alt+B' or 'Ctrl+Alt+Button1 Click'</ShortCut>
          <Description>Implementation(s)</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rebuild '&lt;default&gt;'">
        <Item>
          <IdeaActionID>Compile</IdeaActionID>
          <ShortCut>'Ctrl+Shift+F9'</ShortCut>
          <Description>Rebuild '&lt;default&gt;'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'Main'">
        <Item>
          <IdeaActionID>Run</IdeaActionID>
          <ShortCut>'Shift+F10'</ShortCut>
          <Description>Run 'Main'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'Test1.main()'">
        <Item>
          <IdeaActionID>RunClass</IdeaActionID>
          <ShortCut>'Ctrl+Shift+F10'</ShortCut>
          <Description>Run 'Test1.main()'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Settings...">
        <Item>
          <IdeaActionID>ShowSettings</IdeaActionID>
          <ShortCut>'Ctrl+Alt+S'</ShortCut>
          <Description>Settings...</Description>
          <Count>9</Count>
        </Item>
      </Statistic>
      <Statistic Action="Synchronize">
        <Item>
          <IdeaActionID>Synchronize</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Y'</ShortCut>
          <Description>Synchronize</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="编码规约扫描">
        <Item>
          <IdeaActionID>AliP3CInspectionAction</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Shift+J'</ShortCut>
          <Description>编码规约扫描</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>